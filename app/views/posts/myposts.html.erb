<%= stylesheet_link_tag("post") %>
<p style="color: green"><%= notice %></p>
<div id="posts" class="text-center">
  <%= link_to "New post", new_post_path, class: "btn btn-primary"%>
  <% @posts.each do |post| %>
  
    <% if post.user.id == current_user.id %>
            <%= render post %>
    <% end %>
  <% end %>
</div>

